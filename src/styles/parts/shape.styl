.shape
	display inline-block
	vertical-align middle
	line-height inherit
	fill currentColor

	&-defs
		visibility hidden
		position absolute
		width 0
		height 0
		overflow hidden

	// Shapes default dimensions
	&-arrow
		size 12px 7px

	&-projects-icon
		size 72px 45px

	&-logo-darujme
		size 30px 25px

